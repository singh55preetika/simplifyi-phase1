<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_margin="40dp"
    android:layout_height="400dp"
    android:background="@drawable/rectangle_edittext"
    tools:layout_editor_absoluteX="30dp"
    tools:layout_editor_absoluteY="25dp">

    <RelativeLayout
        android:id="@+id/dashboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="35dp"
        android:layout_marginBottom="284dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.228"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setting"
        app:layout_constraintVertical_bias="0.0">


        <RelativeLayout
            android:id="@+id/Dashboardpage"
            android:layout_width="match_parent"
            android:layout_height="266dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1dp"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="121dp">


            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="65dp"
                android:layout_height="60dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/boy"
                app:layout_constraintBottom_toTopOf="@+id/question"
                app:layout_constraintEnd_toStartOf="@+id/textView7"
                app:layout_constraintHorizontal_bias="0.772"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.44" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="230dp"
                android:layout_marginTop="1dp"
                android:background="@drawable/primary_tag"
                android:gravity="center"
                android:text="Medical"
                android:textColor="#fff"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="95dp"
                android:layout_marginTop="20dp"

                android:text="Richard Parker"

                app:layout_constraintVertical_bias="0.368" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="64dp"
                android:layout_height="21dp"
                android:layout_marginLeft="240dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/secondry_tag"
                android:gravity="center"
                android:text="Health"
                android:textColor="#fff" />


            <TextView
                android:id="@+id/question"
                android:layout_width="281dp"
                android:layout_height="69dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="80dp"
                android:layout_marginRight="8dp"
                android:text="@string/text_question" />

            <ImageView
                android:id="@+id/imagequestion"
                android:layout_width="224dp"
                android:layout_height="95dp"
                android:layout_below="@+id/question"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/medicine"
                app:layout_constraintHorizontal_bias="0.457" />


        </RelativeLayout>

        <LinearLayout
            android:layout_width="118dp"
            android:layout_height="49dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="280dp" >

            <Button
                android:id="@+id/x"
                android:layout_width="47dp"
                android:layout_height="43dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="88dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:background="@drawable/telefono"
                android:onClick="pehle"  />

            <Button
                android:id="@+id/y"
                android:layout_width="48dp"
                android:layout_height="43dp"
                android:layout_marginEnd="88dp"
                android:layout_marginRight="88dp"
                android:background="@drawable/icon_cancel" />
        </LinearLayout>
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>